<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="10dp"
        android:background="@null"
        android:paddingEnd="20dp"
        android:paddingStart="20dp">
        <TextView
            android:id="@+id/dialog_thong_tin_sinh_vien_tv_title"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:background="@drawable/corner_16"
            android:gravity="center"
            android:padding="8dp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="25sp"
            android:layout_marginBottom="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Thông tin sinh viên"/>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/dialog_thong_tin_sinh_vien_constrainLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/custom_dialog_1"
            android:paddingTop="10dp"
            android:paddingBottom="40dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/dialog_thong_tin_sinh_vien_tv_title">
            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="Mã Sinh Viên"
                android:textColor="@color/violet"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:layout_marginTop="-10dp"
                android:id="@+id/dialog_thong_tin_sinh_vien_et_MSV"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="PH25430"
                android:textColor="@color/custom_text_focus_2"
                android:textSize="22sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView" />

            <TextView
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tên Sinh Viên"
                android:textColor="@color/violet"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintTop_toBottomOf="@+id/dialog_thong_tin_sinh_vien_et_MSV"
                tools:layout_editor_absoluteX="-3dp" />

            <EditText
                
                android:layout_marginTop="-10dp"
                android:id="@+id/dialog_thong_tin_sinh_vien_et_ten"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Nguyen Gia Minh"
                android:textColor="@color/custom_text_focus_2"
                android:textSize="22sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView2" />

            <TextView
                android:layout_marginTop="10dp"
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Giới Tính"
                android:textColor="@color/violet"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/dialog_thong_tin_sinh_vien_et_ten" />

            <TextView
                android:focusable="false"
                android:id="@+id/dialog_thong_tin_sinh_vien_tv_gioiTinh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Nam"
                android:clickable="false"
                android:textColor="@color/custom_text_focus_2"
                android:textSize="22sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3" />

            <TextView
                android:layout_marginTop="10dp"
                android:id="@+id/textView4_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Số Điện Thoại"
                android:textColor="@color/violet"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/dialog_thong_tin_sinh_vien_tv_gioiTinh" />

            <EditText
                android:inputType="number"
                android:layout_marginTop="-10dp"
                android:id="@+id/dialog_thong_tin_sinh_vien_et_SDT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="0392747017"
                android:textColor="@color/custom_text_focus_2"
                android:textSize="22sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView4_1" />

            <TextView
                android:layout_marginTop="10dp"
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Ngày Sinh"
                android:textColor="@color/violet"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/dialog_thong_tin_sinh_vien_et_SDT" />

            <EditText
                android:focusable="false"
                android:layout_marginTop="-10dp"
                android:id="@+id/dialog_thong_tin_sinh_vien_et_ngaySinh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="16/09/2003"

                android:textColor="@color/custom_text_focus_2"
                android:textSize="22sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView4" />

            <TextView
                android:layout_marginTop="10dp"
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Lớp Học"
                android:textColor="@color/violet"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/dialog_thong_tin_sinh_vien_et_ngaySinh" />

            <Spinner
                android:id="@+id/dialog_thong_tin_sinh_vien_sp_lopHoc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/custom_text_focus_2"
                android:textSize="22sp"
                app:backgroundTint="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView5" />

            <TextView
                android:layout_marginTop="5dp"
                android:id="@+id/dialog_thong_tin_sinh_vien_tv_ketQuaHocTap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/cantata_one"
                android:text="->Xem Kết Quả Học Tập"
                android:textColor="#C13002"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/dialog_thong_tin_sinh_vien_sp_lopHoc" />

            <View
                android:id="@+id/view"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginEnd="170dp"
                android:background="@color/violet"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/dialog_thong_tin_sinh_vien_tv_ketQuaHocTap" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="-20dp"
            android:gravity="center_horizontal"
            android:weightSum="3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dialog_thong_tin_sinh_vien_constrainLayout">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/dialog_thong_tin_sinh_vien_btn_suaLuu"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.9"
                android:background="@drawable/btn_2"
                android:drawableLeft="@drawable/fix_30"
                android:fontFamily="@font/cantata_one"
                android:paddingStart="10dp"
                android:text="Sửa"
                android:textAllCaps="false"
                android:textColor="#030F87"
                android:textSize="20sp"
                android:textStyle="bold" />
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/dialog_thong_tin_sinh_vien_btnImg_huy"
        android:layout_marginStart="-30dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/cancel_40"
        android:layout_gravity="end"
        android:layout_marginEnd="10dp"
        android:background="@null"/>
</FrameLayout>